modules = ["web", "python-3.11"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["SDL2", "SDL2_image", "SDL2_mixer", "SDL2_ttf", "fontconfig", "freetype", "libjpeg", "libpng", "pkg-config", "portmidi"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Frisbee Game"

[[workflows.workflow]]
name = "Frisbee Game"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python game.py"

[workflows.workflow.metadata]
outputType = "vnc"

[deployment]
deploymentTarget = "vm"
run = ["python", "game.py"]
